{
    "version": 2,
    "name": "your-project-name",
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "user-service/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "user-service/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  